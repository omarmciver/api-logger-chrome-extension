{
  "manifest_version": 3,
  "name": "API Logger",
  "version": "1.1.2",
  "description": "Record API calls and export for analysis. Session management with resume, delete, and JSONL export.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "API Logger",
    "default_popup": "public/popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "devtools_page": "devtools/devtools.html"
}
