{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome with Extension",
      "type": "chrome",
      "request": "launch",
      "url": "chrome://extensions/",
      "runtimeArgs": [
        "--auto-open-devtools-for-tabs"
      ],
      "userDataDir": "${env:TEMP}/api-logger-chrome-profile"
    }
  ]
}
